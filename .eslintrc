{
  "ecmaFeatures": {
    "templateStrings": true
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "camelcase": 0,
    "curly": [ 0, "multi" ],
    "key-spacing": [ 2, { "align": "value" } ],
    "no-shadow": 0,
    "no-use-before-define": [ 2, "nofunc" ],
    "no-underscore-dangle": 0,
    "quotes": [ 2, "single" ],
    "semi": [ 2, "always" ],
    "strict": [ 2, "never" ]
  }
}
